{
    "parserOptions": {
        "ecmaVersion": 5
    },
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "spaced-comment": "off",
        "no-var": "off",
        "max-len": [
            "warn",
            10000
        ],
        "space-before-function-paren": "off",
        "padded-blocks": "off",
        "comma-dangle": [
            "error",
            "never"
        ],
        "new-cap": "off",
        "camelcase": "off",
        "no-invalid-this": "off",
        "key-spacing": [
            "warn",
            {
                "afterColon": true,
                "mode": "minimum"
            }
        ],
        "prefer-rest-params": "off",
        "prefer-spread": "off"
    }
}