{
    "defaultRenderer": "fiori2",
    "renderers": {
        "fiori2": {
            "componentData": {
                "config": {
                    "search": "hidden"
                }
            }
        }
    },

    "bootstrapPlugins":{
        "IssueLogger":{
            "component":"be.FiddleWookie.IssueLogger",
            "url":"plugins/sample/issueLogger",
            "sap-ushell-plugin-type":"RendererExtensions",
            "enabled":true
        }
    },

    "applications": {
        "jsdoc-display": {
            "additionalInformation": "SAPUI5.Component=be.fiddle.jsdoc",
            "applicationType": "URL",
            "url": "./",
            "title": "JSDoco application",
            "navigationMode": "embedded"
        }
    },

    "services":{
        "PluginManager":{
            
        },
        "LaunchPage":{
            "adapter":{
                "config":{
                    "catalogs":[ 
                        {
                            "id":"Documentation_catalog",
                            "title":"UI5 Doco tools",
                            "tiles":[ 
                                {
                                    "id":"JSDOCO",
                                    "size":"1x1",
                                    "tileType": "sap.ushell.ui.tile.StaticTile",
                                    "title": "JSDoco app",
                                    "properties":{
                                        "targetURL":"#jsdoc-display",
                                        "icon":"sap-icon://inbox",
                                        "subtitle": "API doco",
                                        "title": "JSDoco app"                            
                                    }
                                }
                            ]
                        }
                    ],
                    "groups":[ 
                        {
                            "id":"Documentation_group",
                            "title":"UI5 Doco tools",
                            "isPreset":true,
                            "isVisible":true,
                            "isGroupLocked":true,
                            "tiles":[
                                {
                                    "id":"JSDOCO",
                                    "size":"1x1",
                                    "tileType": "sap.ushell.ui.tile.StaticTile",
                                    "title": "JSDoco app",
                                    "properties":{
                                        "targetURL":"#jsdoc-display",
                                        "icon":"sap-icon://inbox",
                                        "subtitle": "API doco",
                                        "title": "JSDoco app"                            
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "ClientSideTargetResolution":{
            "adapter":{
                "config":{
                    "inbounds":{
                        "pluginSample": {
                            "resolutionResult":{
                                "additionalInformation":"SAPUI5.Component=be.FiddleWookie.IssueLogger"
                            },
                            "config": { }
                        }
                    }
                }
            }
        }
    }    
}